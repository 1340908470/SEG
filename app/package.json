    "@umijs/preset-react": "1.x",
    "basscss": "^8.1.0",
    "basscss-flexbox": "^1.0.2",
    "basscss-layout": "^3.1.0",
    "basscss-margin": "^1.0.9",
    "basscss-padding": "^1.1.3",
    "basscss-position": "^2.0.3",
    "dayjs": "^1.10.4",
    "esdk-obs-browserjs": "^3.20.7",
    "flyio": "^0.6.14",
    "pubsub-ts": "^0.2.0",
    "react-jdenticon": "^0.0.9",
    "react-markdown": "^5.0.3",
