    "@types/react-dom": "^16.9.0",
    "@types/validator": "^13.1.3",
    "@umijs/test": "^3.3.7",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.1.0",
    "prettier": "2.2.1",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
